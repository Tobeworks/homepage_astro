    <div class="row">
      <div class="col-sm align-self-center title-lead">
        <h2 class="cover my-2 text-center">Was mache ich so?</h2>
        <p class="lead text-center w-75 mx-auto my-3">
          Ein kleine Liste an ausgewählten Projekten
        </p>
      </div>
    </div>
    <a href="#!" class="title-arrow-down">
      <i class="fa-solid fa-arrow-down"></i>
    </a>
  </section>

  <!-- Migrosbank Start -->
  <section class="bg-silver portfolio-row" id="contentStart">
    <div class="card mb-3">
      <div class="row align-items-center">
        <div class="col-sm-7">
          <Picture
            class="card-img-top py-5"
